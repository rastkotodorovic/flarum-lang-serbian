{
    "name": "rastkotodorovic/flarum-lang-serbian",
    "type": "flarum-extension",
    "description": "Serbian language pack.",
    "homepage": "https://github.com/rastkotodorovic/flarum-lang-serbian",
    "keywords": ["locale","translation","srpski","serbian"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rastkotodorovic/flarum-lang-serbian/issues",
        "source": "https://github.com/rastkotodorovic/flarum-lang-serbian"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.9.x-dev"
        },
        "flarum-extension": {
            "title": "Srpski",
            "category": "language",
            "info": {
                "website": "https://github.com/rastkotodorovic/flarum-lang-serbian",
                "support": "https://github.com/rastkotodorovic/flarum-lang-serbian/issues"
            },
            "icon": {
                "image": "flag.jpg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }

        },
        "flarum-locale": {
            "code": "sr",
            "title": "Srpski"
        }
    }
}
